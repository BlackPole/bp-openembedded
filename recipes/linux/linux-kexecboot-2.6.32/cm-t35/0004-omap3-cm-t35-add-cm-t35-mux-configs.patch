+{
+	/* nCS and IRQ mux for CM-T35 ethernet */
+	omap_cfg_reg(G5_34XX_GPMC_NCS5);
+	omap_cfg_reg(A23_34XX_GPIO163_UP);
+
+	/* nCS, IRQ and reset mux for SB-T35 ethernet */
